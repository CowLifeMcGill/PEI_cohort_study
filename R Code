                                      p = c(train = 0.80, valid = 0.20),
                                      seed = 1801)
  
  wd.mv.imput.train.split <- rbind(wd.mv.imput.train.split,
                                   data[splitIndex$train,])
  
  wd.mv.imput.val.split <- rbind(wd.mv.imput.val.split,
                                 data[splitIndex$valid,])
  
